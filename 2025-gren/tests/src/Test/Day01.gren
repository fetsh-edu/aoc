module Test.Day01 exposing (tests)

import Test exposing (Test, describe, test)
import Expect
import Day01

input = """
L68
L30
R48
L5
R60
L55
L1
L99
R14
L82
"""

tests : Test
tests =
    describe "Day 01"
        [ test "Part 1 with example input" <|
            \_ ->
                let
                    result =
                        Day01.solve input
                in
                Expect.equal "3" result.part1
        , test "Part 2 with example input" <|
            \_ ->
                let
                    result =
                        Day01.solve input
                in
                Expect.equal "6" result.part2
        ]
