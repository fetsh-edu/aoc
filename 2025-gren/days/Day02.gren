module Day02 exposing (solve)

solve : String -> { part1 : String, part2 : String }
solve input =
    let
        parsedInput = parseInput input
    in
    { part1 = solvePart1 parsedInput |> String.fromInt
    , part2 = solvePart2 parsedInput |> String.fromInt
    }

parseInput : String -> Array Int
parseInput input =
    input |> String.trim |> String.split ","

solvePart1 : Array Int -> Int
solvePart1 input = 2

solvePart2 : Array Int -> Int
solvePart2 input = 3
